model Favorite{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  proj_abbr_name_list  String[]
}
model Page1compareinfomation{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_abbr_name  String @unique
  fundType String
  risk_spectrum String
  Feeder_Fund String
  Currency_policy String
  Dividend_payment_policy String
  Sales_fees String
  Buyback_fee String
  Mutual_fund_expenses String
  Minimum_initial_investment String
  Minimum_next_investment String
  Investment_registration_date String
  Net_asset_value String
}
model Page2Operating_results_and_dividends{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name  String@unique
}
model Page3topfiveCompareport{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name  String @unique
  top_five_holding Json
}
model Page3typeCompareport{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name  String @unique
  type_data Json
}
model Page3Investment_proportionCompareport{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String @unique
  proj_abbr_name  String @unique
  Investment_proportion_data Json
}
model Page4Fee{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String @unique
  proj_abbr_name  String @unique
  datafromsheet Json
  datafromReal Json
}

