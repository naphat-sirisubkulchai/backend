
generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Product {
  id              String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id         String    @unique
  proj_abbr_name  String    @unique
  proj_name_en    String
  proj_name_th    String
  unique_id       String
  Allinfo         Json
}

model Favorite {
  id                   String    @id @default(auto()) @map("_id") @db.ObjectId
  user                 String    @unique
  proj_abbr_name_list  String[]
  product_json_list Json[]
}


model Page1compareinfomation{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name  String@unique
  compareinfomation Json
}

model Page2Operating_results_and_dividends{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name  String@unique
  operating_results Json
}
model Page3topfiveCompareport{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name  String @unique
  top_five_holding Json
}
model Page3typeCompareport{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name  String @unique
  type_data Json
}
model Page3Investment_proportionCompareport{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String @unique
  proj_abbr_name  String @unique
  Investment_proportion_data Json
}
model Page4Fee{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String @unique
  proj_abbr_name  String @unique
  datafromsheet Json
  datafromReal Json
}

model AllProductInfo{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  proj_id String    @unique
  proj_abbr_name String @unique
  Allinfo Json
}
